{
  "expo": {
    "name": "StatLine",
    "slug": "statline",
    "owner": "akanel",
    "scheme": "statline",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akanel.StatLine",
      "buildNumber": "1",
      "appStoreUrl": "https://apps.apple.com/app/statline/id6758228140",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSSupportsOpeningDocumentsInPlace": false,
        "NSPhotoLibraryUsageDescription": "Allow StatLine to access your photos to select team and player images",
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "StatLine Export",
            "CFBundleTypeRole": "Editor",
            "LSHandlerRank": "Owner",
            "LSItemContentTypes": ["com.akanel.statline.export"]
          }
        ],
        "UTExportedTypeDeclarations": [
          {
            "UTTypeIdentifier": "com.akanel.statline.export",
            "UTTypeDescription": "StatLine Game Data",
            "UTTypeConformsTo": ["public.json"],
            "UTTypeTagSpecification": {
              "public.filename-extension": ["statline"]
            }
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.akanel.StatLine"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "./plugins/fix-hermes-warning.js",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow StatLine to access your photos to select team and player images",
          "cameraPermission": false,
          "microphonePermission": false
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73e9210c-f423-4718-848c-ae00dab72485"
      }
    }
  }
}
